{
  "rewrites": [
    {
      "source": "/api/Voter/(.*)",
      "destination": "https://xtend.online/Voter/$1"
    }
  ],
  "functions": {
    "api/whatsapp-send.js": {
      "runtime": "nodejs18.x"
    }
  },
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
